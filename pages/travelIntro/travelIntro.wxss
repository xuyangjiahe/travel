/* pages/travelIntro/travelIntro.wxss */
.intro {
  width: 750rpx;
  height: 100%;
  box-sizing: border-box;
  padding-top:100rpx;
  padding: 100rpx 30rpx 30rpx 30rpx;
}
.intro_logo {
  display: block;
  width:164rpx;
  height:164rpx;
  border-radius:8rpx;
  margin: 0 auto;
}
.intro_name {
  display: block;
  width: 100%;
  height: 70rpx;
  text-align: center;
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  line-height:70rpx;
  margin: 0 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.intor_info {
  margin-top: 50rpx;

}
.intor_title {
  width: 100%;
  height:70rpx;
  font-size:36rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
  line-height:70rpx;
  text-align: left;
}
.intor_com {
  display: block;
  width:100%;
  height:auto;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  line-height:66rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
}